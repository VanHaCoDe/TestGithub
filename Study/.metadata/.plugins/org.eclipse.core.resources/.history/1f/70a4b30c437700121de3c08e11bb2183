		// TODO Auto-generated method stub
		super.onCreate();
	}
	@Override
	public void onDestroy() {
		// TODO Auto-generated method stub
		super.onDestroy();
	}
	@Override
	public void onStart(Intent intent, int startId) {
		// TODO Auto-generated method stub
	}
}
